{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mtlog",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_template_content"
      }
    },
    "_template_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "go_property"
        },
        {
          "type": "SYMBOL",
          "name": "builtin_property"
        },
        {
          "type": "SYMBOL",
          "name": "property"
        },
        {
          "type": "SYMBOL",
          "name": "literal_text"
        }
      ]
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "hint",
              "content": {
                "type": "SYMBOL",
                "name": "hint_symbol"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "_property_name"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "format",
              "content": {
                "type": "SYMBOL",
                "name": "format_spec"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "go_property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "_property_name"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "builtin_property": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "_property_name"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "format",
              "content": {
                "type": "SYMBOL",
                "name": "format_spec"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "hint_symbol": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "STRING",
          "value": "$"
        }
      ]
    },
    "_property_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "dotted_name"
        },
        {
          "type": "SYMBOL",
          "name": "numeric_index"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]*"
    },
    "dotted_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "numeric_index": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "format_spec": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "format_string",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "TOKEN",
              "content": {
                "type": "PATTERN",
                "value": "[^}]+"
              }
            },
            "named": true,
            "value": "identifier"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\r?\\n"
      }
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "literal_text"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}