{
  "name": "tree-sitter-mtlog",
  "version": "0.1.0",
  "main": "bindings/node",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "tree-sitter test",
    "test:update": "tree-sitter test --update",
    "generate": "tree-sitter generate",
    "build": "node-gyp rebuild",
    "install": "tree-sitter generate && node-gyp rebuild",
    "parse": "tree-sitter parse",
    "highlight": "tree-sitter highlight",
    "benchmark": "tree-sitter test 2>&1 | grep 'average speed'"
  },
  "keywords": [
    "tree-sitter",
    "mtlog",
    "parser",
    "syntax-highlighting"
  ],
  "author": "Brandon Williams",
  "license": "MIT",
  "description": "Tree-sitter grammar for mtlog message templates",
  "tree-sitter": [
    {
      "scope": "source.mtlog",
      "injection-regex": "^mtlog$",
      "file-types": ["mtlog"]
    }
  ],
  "dependencies": {
    "nan": "^2.23.0",
    "tree-sitter": "^0.25.0",
    "tree-sitter-cli": "^0.25.8"
  }
}
