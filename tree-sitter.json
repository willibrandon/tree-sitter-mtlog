{
  "grammars": [
    {
      "name": "mtlog",
      "camelcase": "Mtlog",
      "scope": "source.mtlog",
      "path": ".",
      "file-types": ["mtlog"],
      "injection-regex": "^mtlog$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "textobjects": "queries/textobjects.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for mtlog message template syntax",
    "author": "Brandon Willis",
    "homepage": "https://github.com/willibrandon/tree-sitter-mtlog",
    "repository": "https://github.com/willibrandon/tree-sitter-mtlog.git",
    "performance": {
      "parse_time_100_chars_ms": 0.016,
      "parse_time_1000_chars_ms": 0.16,
      "average_speed_bytes_per_ms": 6370,
      "test_suite_success_rate": "100%"
    },
    "compatibility": {
      "tree-sitter": ">=0.20.0",
      "node": ">=14.0.0"
    },
    "features": [
      "syntax-highlighting",
      "text-objects",
      "structural-navigation",
      "error-recovery",
      "external-scanner",
      "unicode-support"
    ]
  }
}